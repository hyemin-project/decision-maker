{"ast":null,"code":"import { ref, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"EnterChoicesStep\",\n  setup() {\n    const store = useStore();\n    const newChoice = ref(\"\");\n    const choices = computed(() => store.state.choices);\n    const addChoice = () => {\n      if (newChoice.value) {\n        store.commit(\"addChoice\", newChoice.value);\n        newChoice.value = \"\";\n      }\n    };\n    const removeChoice = index => {\n      store.commit(\"removeChoice\", index);\n    };\n    const nextStep = () => {\n      console.log(\"Next step called\");\n      store.commit(\"setCurrentStep\", 3);\n      console.log(\"Current step after commit:\", store.state.currentStep);\n    };\n    return {\n      newChoice,\n      choices,\n      addChoice,\n      removeChoice,\n      nextStep\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","name","setup","store","newChoice","choices","state","addChoice","value","commit","removeChoice","index","nextStep","console","log","currentStep"],"sources":["/Users/hyemin/Documents/Projects/decision-maker/client/src/components/EnterChoicesStep.vue"],"sourcesContent":["<template>\n  <div class=\"enter-choices-step\">\n    <h2>Enter Your Choices</h2>\n    <div class=\"input-group\">\n      <input\n        v-model=\"newChoice\"\n        @keyup.enter=\"addChoice\"\n        placeholder=\"Enter a choice\"\n      />\n      <button @click=\"addChoice\" :disabled=\"!newChoice\">Add</button>\n    </div>\n    <ul class=\"choices-list\">\n      <li v-for=\"(choice, index) in choices\" :key=\"index\">\n        {{ choice }}\n        <button @click=\"removeChoice(index)\">Remove</button>\n      </li>\n    </ul>\n    <button @click=\"nextStep\" :disabled=\"choices.length < 2\">Next</button>\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  name: \"EnterChoicesStep\",\n  setup() {\n    const store = useStore();\n    const newChoice = ref(\"\");\n    const choices = computed(() => store.state.choices);\n\n    const addChoice = () => {\n      if (newChoice.value) {\n        store.commit(\"addChoice\", newChoice.value);\n        newChoice.value = \"\";\n      }\n    };\n\n    const removeChoice = (index) => {\n      store.commit(\"removeChoice\", index);\n    };\n\n    const nextStep = () => {\n      console.log(\"Next step called\");\n      store.commit(\"setCurrentStep\", 3);\n      console.log(\"Current step after commit:\", store.state.currentStep);\n    };\n\n    return {\n      newChoice,\n      choices,\n      addChoice,\n      removeChoice,\n      nextStep,\n    };\n  },\n};\n</script>"],"mappings":"AAsBA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC,CAAC;IACxB,MAAMI,SAAQ,GAAIN,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMO,OAAM,GAAIN,QAAQ,CAAC,MAAMI,KAAK,CAACG,KAAK,CAACD,OAAO,CAAC;IAEnD,MAAME,SAAQ,GAAIA,CAAA,KAAM;MACtB,IAAIH,SAAS,CAACI,KAAK,EAAE;QACnBL,KAAK,CAACM,MAAM,CAAC,WAAW,EAAEL,SAAS,CAACI,KAAK,CAAC;QAC1CJ,SAAS,CAACI,KAAI,GAAI,EAAE;MACtB;IACF,CAAC;IAED,MAAME,YAAW,GAAKC,KAAK,IAAK;MAC9BR,KAAK,CAACM,MAAM,CAAC,cAAc,EAAEE,KAAK,CAAC;IACrC,CAAC;IAED,MAAMC,QAAO,GAAIA,CAAA,KAAM;MACrBC,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;MAC/BX,KAAK,CAACM,MAAM,CAAC,gBAAgB,EAAE,CAAC,CAAC;MACjCI,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEX,KAAK,CAACG,KAAK,CAACS,WAAW,CAAC;IACpE,CAAC;IAED,OAAO;MACLX,SAAS;MACTC,OAAO;MACPE,SAAS;MACTG,YAAY;MACZE;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}