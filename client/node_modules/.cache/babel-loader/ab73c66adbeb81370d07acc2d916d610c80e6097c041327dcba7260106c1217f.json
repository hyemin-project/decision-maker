{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport api from '@/services/api';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: 'ResultsStep',\n  setup() {\n    const store = useStore();\n    const result = computed(() => store.state.result);\n    const choices = computed(() => store.state.choices);\n    const showShareOptions = ref(false);\n    const isSaving = ref(false);\n    const showSuccessModal = ref(false);\n    const isLoggedIn = computed(() => !!store.state.user);\n    const router = useRouter();\n    const userId = computed(() => store.state.user?.id || null);\n    const resultColor = computed(() => {\n      const colors = store.state.wheelCustomization.colors || [];\n      const index = choices.value.indexOf(result.value);\n      return colors[index] || '#4caf50'; // default color if not found\n    });\n    const spinAgain = () => {\n      store.commit('setCurrentStep', 5); // Go back to spin wheel step\n    };\n    const newDecision = () => {\n      store.commit('resetDecision');\n      store.commit('setCurrentStep', 1); // Go back to first step\n    };\n    const shareResult = () => {\n      showShareOptions.value = !showShareOptions.value;\n    };\n    const shareVia = platform => {\n      const message = `I used the Advanced Virtual Decision Maker and got: ${result.value}`;\n      let url = '';\n      switch (platform) {\n        case 'twitter':\n          url = `https://twitter.com/intent/tweet?text=${encodeURIComponent(message)}`;\n          break;\n        case 'facebook':\n          url = `https://www.facebook.com/sharer/sharer.php?u=${encodeURIComponent(window.location.href)}&quote=${encodeURIComponent(message)}`;\n          break;\n        case 'email':\n          url = `mailto:?subject=My Decision Result&body=${encodeURIComponent(message)}`;\n          break;\n      }\n      if (url) {\n        window.open(url, '_blank');\n      }\n      showShareOptions.value = false;\n    };\n    const saveResult = async () => {\n      if (isSaving.value) return;\n      isSaving.value = true;\n      try {\n        const savedResult = await api.saveResult({\n          result: result.value,\n          choices: choices.value,\n          userId: userId.value\n        });\n        console.log('Result saved:', savedResult);\n        showSuccessModal.value = true;\n      } catch (error) {\n        console.error('Error saving result:', error);\n        alert('Failed to save result. Please try again.');\n      } finally {\n        isSaving.value = false;\n      }\n    };\n    const promptLogin = () => {\n      router.push('/login');\n    };\n    const closeModal = () => {\n      showSuccessModal.value = false;\n    };\n    return {\n      result,\n      choices,\n      resultColor,\n      showShareOptions,\n      spinAgain,\n      newDecision,\n      shareResult,\n      shareVia,\n      saveResult,\n      isSaving,\n      showSuccessModal,\n      isLoggedIn,\n      promptLogin,\n      closeModal\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","api","useRouter","name","setup","store","result","state","choices","showShareOptions","isSaving","showSuccessModal","isLoggedIn","user","router","userId","id","resultColor","colors","wheelCustomization","index","value","indexOf","spinAgain","commit","newDecision","shareResult","shareVia","platform","message","url","encodeURIComponent","window","location","href","open","saveResult","savedResult","console","log","error","alert","promptLogin","push","closeModal"],"sources":["/Users/hyemin/Documents/Projects/decision-maker/client/src/components/ResultsStep.vue"],"sourcesContent":["<template>\n  <div class=\"container results-step\">\n    <div class=\"row justify-content-center\">\n      <div class=\"col-12 text-center\">\n        <h2>Your Decision Result</h2>\n      </div>\n      <div class=\"col-12 col-md-8 col-lg-6\">\n        <div class=\"result-display\" :style=\"{ backgroundColor: resultColor }\">\n          <p class=\"result-text\">{{ result }}</p>\n        </div>\n      </div>\n      <div class=\"col-12 text-center mt-3 action-buttons\">\n        <button class=\"btn btn-primary me-2\" @click=\"spinAgain\">Spin Again</button>\n        <button class=\"btn btn-warning me-2\" @click=\"newDecision\">New Decision</button>\n        <button class=\"btn btn-success me-2\" @click=\"shareResult\">Share Result</button>\n        <button v-if=\"isLoggedIn\" class=\"btn btn-info\" @click=\"saveResult\" :disabled=\"isSaving\">\n          {{ isSaving ? 'Saving...' : 'Save Result' }}\n        </button>\n        <button v-else class=\"btn btn-secondary\" @click=\"promptLogin\">Login to Save</button>\n      </div>\n      <div v-if=\"showShareOptions\" class=\"col-12 text-center mt-3 share-options\">\n        <button class=\"btn btn-outline-primary me-2\" @click=\"shareVia('twitter')\">Twitter</button>\n        <button class=\"btn btn-outline-primary me-2\" @click=\"shareVia('facebook')\">Facebook</button>\n        <button class=\"btn btn-outline-primary\" @click=\"shareVia('email')\">Email</button>\n      </div>\n      <div class=\"col-12 col-md-8 col-lg-6 mt-3 decision-summary\">\n        <h3>Decision Summary</h3>\n        <ul class=\"list-group\">\n          <li v-for=\"(choice, index) in choices\" :key=\"index\" class=\"list-group-item\">\n            {{ choice }} <span v-if=\"choice === result\" class=\"badge bg-success\">Selected</span>\n          </li>\n        </ul>\n      </div>\n    </div>\n\n    <!-- Success Modal -->\n    <b-modal v-model=\"showSuccessModal\" title=\"Result Saved\" ok-only @ok=\"closeModal\">\n      <p class=\"my-4\">Your decision result has been saved successfully!</p>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from 'vue'\nimport { useStore } from 'vuex'\nimport api from '@/services/api' \nimport { useRouter } from 'vue-router' \n\nexport default {\n  name: 'ResultsStep',\n  setup() {\n    const store = useStore()\n    const result = computed(() => store.state.result)\n    const choices = computed(() => store.state.choices)\n    const showShareOptions = ref(false)\n    const isSaving = ref(false)\n    const showSuccessModal = ref(false)\n    const isLoggedIn = computed(() => !!store.state.user)\n    const router = useRouter()\n    const userId = computed(() => store.state.user?.id || null)\n\n    const resultColor = computed(() => {\n      const colors = store.state.wheelCustomization.colors || []\n      const index = choices.value.indexOf(result.value)\n      return colors[index] || '#4caf50' // default color if not found\n    })\n\n    const spinAgain = () => {\n      store.commit('setCurrentStep', 5) // Go back to spin wheel step\n    }\n\n    const newDecision = () => {\n      store.commit('resetDecision')\n      store.commit('setCurrentStep', 1) // Go back to first step\n    }\n\n    const shareResult = () => {\n      showShareOptions.value = !showShareOptions.value\n    }\n\n    const shareVia = (platform) => {\n      const message = `I used the Advanced Virtual Decision Maker and got: ${result.value}`\n      let url = ''\n\n      switch (platform) {\n        case 'twitter':\n          url = `https://twitter.com/intent/tweet?text=${encodeURIComponent(message)}`\n          break\n        case 'facebook':\n          url = `https://www.facebook.com/sharer/sharer.php?u=${encodeURIComponent(window.location.href)}&quote=${encodeURIComponent(message)}`\n          break\n        case 'email':\n          url = `mailto:?subject=My Decision Result&body=${encodeURIComponent(message)}`\n          break\n      }\n\n      if (url) {\n        window.open(url, '_blank')\n      }\n\n      showShareOptions.value = false\n    }\n\n    const saveResult = async () => {\n      if (isSaving.value) return\n      isSaving.value = true\n      try {\n        const savedResult = await api.saveResult({\n          result: result.value,\n          choices: choices.value,\n          userId: userId.value\n        })\n        console.log('Result saved:', savedResult)\n        showSuccessModal.value = true\n      } catch (error) {\n        console.error('Error saving result:', error)\n        alert('Failed to save result. Please try again.')\n      } finally {\n        isSaving.value = false\n      }\n    }\n\n    const promptLogin = () => {\n      router.push('/login')\n    }\n\n    const closeModal = () => {\n      showSuccessModal.value = false\n    }\n\n    return {\n      result,\n      choices,\n      resultColor,\n      showShareOptions,\n      spinAgain,\n      newDecision,\n      shareResult,\n      shareVia,\n      saveResult,\n      isSaving,\n      showSuccessModal,\n      isLoggedIn,\n      promptLogin,\n      closeModal\n    }\n  }\n}\n</script>"],"mappings":";AA2CA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,GAAE,MAAO,gBAAe;AAC/B,SAASC,SAAQ,QAAS,YAAW;AAErC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC;IACvB,MAAMM,MAAK,GAAIP,QAAQ,CAAC,MAAMM,KAAK,CAACE,KAAK,CAACD,MAAM;IAChD,MAAME,OAAM,GAAIT,QAAQ,CAAC,MAAMM,KAAK,CAACE,KAAK,CAACC,OAAO;IAClD,MAAMC,gBAAe,GAAIX,GAAG,CAAC,KAAK;IAClC,MAAMY,QAAO,GAAIZ,GAAG,CAAC,KAAK;IAC1B,MAAMa,gBAAe,GAAIb,GAAG,CAAC,KAAK;IAClC,MAAMc,UAAS,GAAIb,QAAQ,CAAC,MAAM,CAAC,CAACM,KAAK,CAACE,KAAK,CAACM,IAAI;IACpD,MAAMC,MAAK,GAAIZ,SAAS,CAAC;IACzB,MAAMa,MAAK,GAAIhB,QAAQ,CAAC,MAAMM,KAAK,CAACE,KAAK,CAACM,IAAI,EAAEG,EAAC,IAAK,IAAI;IAE1D,MAAMC,WAAU,GAAIlB,QAAQ,CAAC,MAAM;MACjC,MAAMmB,MAAK,GAAIb,KAAK,CAACE,KAAK,CAACY,kBAAkB,CAACD,MAAK,IAAK,EAAC;MACzD,MAAME,KAAI,GAAIZ,OAAO,CAACa,KAAK,CAACC,OAAO,CAAChB,MAAM,CAACe,KAAK;MAChD,OAAOH,MAAM,CAACE,KAAK,KAAK,SAAQ,EAAE;IACpC,CAAC;IAED,MAAMG,SAAQ,GAAIA,CAAA,KAAM;MACtBlB,KAAK,CAACmB,MAAM,CAAC,gBAAgB,EAAE,CAAC,GAAE;IACpC;IAEA,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxBpB,KAAK,CAACmB,MAAM,CAAC,eAAe;MAC5BnB,KAAK,CAACmB,MAAM,CAAC,gBAAgB,EAAE,CAAC,GAAE;IACpC;IAEA,MAAME,WAAU,GAAIA,CAAA,KAAM;MACxBjB,gBAAgB,CAACY,KAAI,GAA