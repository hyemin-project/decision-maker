{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'SetProbabilitiesStep',\n  setup() {\n    const store = useStore();\n    const choices = computed(() => store.state.choices);\n    const probabilities = ref(choices.value.map(() => 100 / choices.value.length));\n    const normalizeProbabilities = () => {\n      const total = probabilities.value.reduce((sum, prob) => sum + Number(prob), 0);\n      probabilities.value = probabilities.value.map(prob => prob / total * 100);\n    };\n    const nextStep = () => {\n      store.commit('setProbabilities', probabilities.value);\n      store.commit('setCurrentStep', 4);\n    };\n    return {\n      choices,\n      probabilities,\n      normalizeProbabilities,\n      nextStep\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","name","setup","store","choices","state","probabilities","value","map","length","normalizeProbabilities","total","reduce","sum","prob","Number","nextStep","commit"],"sources":["/Users/hyemin/Documents/Projects/decision-maker/client/src/components/SetProbabilitiesStep.vue"],"sourcesContent":["<template>\n  <div class=\"set-probabilities-step\">\n    <h2>Set Probabilities (Optional)</h2>\n    <div v-for=\"(choice, index) in choices\" :key=\"index\" class=\"probability-input\">\n      <label>{{ choice }}</label>\n      <input type=\"range\" v-model=\"probabilities[index]\" min=\"0\" max=\"100\" @input=\"normalizeProbabilities\">\n      <span>{{ probabilities[index] }}%</span>\n    </div>\n    <button @click=\"nextStep\">Next</button>\n  </div>\n</template>\n\n<script>\nimport { ref, computed } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default {\n  name: 'SetProbabilitiesStep',\n  setup() {\n    const store = useStore()\n    const choices = computed(() => store.state.choices)\n    const probabilities = ref(choices.value.map(() => 100 / choices.value.length))\n\n    const normalizeProbabilities = () => {\n      const total = probabilities.value.reduce((sum, prob) => sum + Number(prob), 0)\n      probabilities.value = probabilities.value.map(prob => (prob / total) * 100)\n    }\n\n    const nextStep = () => {\n      store.commit('setProbabilities', probabilities.value)\n      store.commit('setCurrentStep', 4)\n    }\n\n    return {\n      choices,\n      probabilities,\n      normalizeProbabilities,\n      nextStep\n    }\n  }\n}\n</script>"],"mappings":"AAaA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;IACvB,MAAMI,OAAM,GAAIL,QAAQ,CAAC,MAAMI,KAAK,CAACE,KAAK,CAACD,OAAO;IAClD,MAAME,aAAY,GAAIR,GAAG,CAACM,OAAO,CAACG,KAAK,CAACC,GAAG,CAAC,MAAM,GAAE,GAAIJ,OAAO,CAACG,KAAK,CAACE,MAAM,CAAC;IAE7E,MAAMC,sBAAqB,GAAIA,CAAA,KAAM;MACnC,MAAMC,KAAI,GAAIL,aAAa,CAACC,KAAK,CAACK,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIE,MAAM,CAACD,IAAI,CAAC,EAAE,CAAC;MAC7ER,aAAa,CAACC,KAAI,GAAID,aAAa,CAACC,KAAK,CAACC,GAAG,CAACM,IAAG,IAAMA,IAAG,GAAIH,KAAK,GAAI,GAAG;IAC5E;IAEA,MAAMK,QAAO,GAAIA,CAAA,KAAM;MACrBb,KAAK,CAACc,MAAM,CAAC,kBAAkB,EAAEX,aAAa,CAACC,KAAK;MACpDJ,KAAK,CAACc,MAAM,CAAC,gBAAgB,EAAE,CAAC;IAClC;IAEA,OAAO;MACLb,OAAO;MACPE,aAAa;MACbI,sBAAsB;MACtBM;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}